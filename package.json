{
  "name": "arcane-ts",
  "version": "1.0.0",
  "description": "Habbo Hotel Emulator - TypeScript/Bun",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "bun": "^1.3.3",
    "chalk": "^5.3.0",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  }
}
